                */

                //circulo
                ctx.beginPath();
                ctx.arc(250,100,50,0,2*Math.PI);
                ctx.strokeStyle= "rgba(200,0,0,0.5)";
                ctx.stroke();

                ctx.beginPath();
                ctx.arc(355,100,50,0,2*Math.PI);
                ctx.fillStyle= "rgb(200,0,0)";
                ctx.fill();

            </script>
